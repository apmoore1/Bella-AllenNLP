{
    "dataset_reader": {
      "type": "target_dataset"
    },
    "train_data_path": "tests/test_data/target_reader_data.json",
    "validation_data_path": "tests/test_data/target_reader_data.json",
    "model": {
      "type": "target_classifier",
      "text_field_embedder": {
        "token_embedders": {
          "tokens": {
            "type": "embedding",
            "embedding_dim": 2,
            "trainable": false
          }
        }
      },
      "target_field_embedder": {
          "token_embedders": {
            "tokens": {
              "type": "embedding",
              "embedding_dim": 2,
              "trainable": false
            }
          }
      },
      "text_encoder": {
        "type": "boe",
        "embedding_dim": 2,
        "averaged": true
      },
      "target_encoder": {
        "type": "boe",
        "embedding_dim": 2,
        "averaged": true
      },
      "classifier_feedforward": {
        "input_dim": 4,
        "num_layers": 2,
        "hidden_dims": [2, 3],
        "activations": ["sigmoid", "linear"],
        "dropout": [0.2, 0.0]
      }
    },
    "iterator": {
      "type": "bucket",
      "sorting_keys": [["target", "num_tokens"], ["text", "num_tokens"]],
      "padding_noise": 0.0,
      "batch_size": 64
    },
  
    "trainer": {
      "num_epochs": 1,
      "cuda_device": -1,
      "grad_clipping": 5.0,
      "validation_metric": "+accuracy",
      "optimizer": {
        "type": "adagrad"
      }
    }
  }