{
    "dataset_reader": {
      "type": "sentence_target_dataset",
      "token_indexers": {
        "tokens": {
            "type": "single_id",
            "lowercase_tokens": true
        }
      }
    },
    "train_data_path": "/home/andrew/.Bella/Datasets/Election Train Group",
    "validation_data_path": "/home/andrew/.Bella/Datasets/Election Val Group",
    "test_data_path": "/home/andrew/.Bella/Datasets/Election Test Group",
    "evaluate_on_test": true,
    "model": {
      "type": "attention_multi_target_classifier",
        "text_field_embedder": {
          "token_embedders": {
              "tokens": {
                "embedding_dim": 10,
                "trainable": true
              }
              
          }
      },
      "text_encoder": {
        "type": "lstm",
        "bidirectional": false,
        "input_size": 10,
        "hidden_size": 10,
        "num_layers": 1
      },
      "target_encoder": {
        "type": "boe",
        "embedding_dim": 10,
        "averaged": true
      },
      "regularizer": [[".*", {"type": "l2", "alpha": 0.0001}]],
      "dropout": 0.5,
      "variational_dropout": 0.5,
      "target_scale": true,
      "context_preserving": true
    },
    "iterator": {
        "type": "bucket",
        "sorting_keys": [["text", "num_tokens"]],
        "padding_noise": 0.1,
        "batch_size": 32
      },
    
    "trainer": {
      "num_epochs": 200,
      "patience": 20,
      "cuda_device": 0,
      "shuffle": true,
      "grad_clipping": 5,
      "optimizer": {
          "type": "sgd",
          "lr": 0.01,
          "momentum": 0.9
      }
    }
  }